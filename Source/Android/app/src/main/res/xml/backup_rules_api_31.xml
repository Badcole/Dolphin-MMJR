<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfNoEncryptionCapabilities="false">
        <exclude
            domain="external"
            path="./Cache/" />

        <exclude
            domain="external"
            path="./Dump/" />
        <exclude
            domain="external"
            path="./Load/" />
        <exclude
            domain="external"
            path="./ResourcePacks/" />
        <include
            domain="external"
            path="." />
    </cloud-backup>

    <device-transfer>
        <exclude
            domain="external"
            path="./Cache/" />

        <include
            domain="external"
            path="." />
    </device-transfer>
</data-extraction-rules>